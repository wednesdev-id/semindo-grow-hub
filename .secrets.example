# Example secrets file for local testing with `act`
# Copy this to .secrets and fill in your values
# NEVER commit .secrets to git

PROD_SERVER_HOST=your-production-server.com
PROD_SERVER_USER=deploy
PROD_SERVER_SSH_KEY="-----BEGIN OPENSSH PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END OPENSSH PRIVATE KEY-----"
PROD_SERVER_PORT=22
PROD_DEPLOY_PATH=/var/www/semindo

STAGING_SERVER_HOST=your-staging-server.com
STAGING_SERVER_USER=deploy
STAGING_SERVER_SSH_KEY="-----BEGIN OPENSSH PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END OPENSSH PRIVATE KEY-----"
STAGING_SERVER_PORT=22
STAGING_DEPLOY_PATH=/var/www/semindo-staging

VITE_API_URL_PROD=https://api.semindo.com/api/v1
VITE_API_URL_STAGING=https://staging-api.semindo.com/api/v1

SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
